        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": { "*": [ "node_modules/*"] },
        "strict": false,
    },
    "include": ["src/**/*", "config", "src/router.js"],
    "exclude": ["node_modules"]
}